<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mwilson.inventoryfx.AddPartOutsourced">
   <children>
      <Label layoutX="64.0" layoutY="43.0" text="Add Part">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <RadioButton fx:id="inHouse" layoutX="180.0" layoutY="45.0" mnemonicParsing="false" onAction="#InHouseSelected" text="In-House" />
      <RadioButton fx:id="outsourced" layoutX="351.0" layoutY="45.0" mnemonicParsing="false" onAction="#OutsourcedSelected" selected="true" text="Outsourced" />
      <Button fx:id="save" layoutX="464.0" layoutY="353.0" mnemonicParsing="false" onAction="#OnSaveClicked" text="Save" />
      <Button fx:id="cancel" layoutX="526.0" layoutY="353.0" mnemonicParsing="false" onAction="#OnCancelClicked" text="Cancel" />
      <TextField fx:id="fieldID" editable="false" layoutX="141.0" layoutY="111.0" text="Auto Generated" />
      <Label layoutX="104.0" layoutY="115.0" text="ID" />
      <TextField fx:id="fieldName" layoutX="141.0" layoutY="146.0" />
      <TextField fx:id="fieldInv" layoutX="141.0" layoutY="182.0" />
      <TextField fx:id="fieldCost" layoutX="141.0" layoutY="223.0" />
      <TextField fx:id="fieldMax" layoutX="141.0" layoutY="262.0" />
      <TextField fx:id="fieldCompanyName" layoutX="141.0" layoutY="300.0" />
      <TextField fx:id="fieldMin" layoutX="390.0" layoutY="262.0" />
      <Label layoutX="85.0" layoutY="151.0" text="Name" />
      <Label layoutX="96.0" layoutY="186.0" text="Inv" />
      <Label layoutX="90.0" layoutY="227.0" text="Cost" />
      <Label layoutX="90.0" layoutY="266.0" text="Max" />
      <Label layoutX="337.0" layoutY="266.0" text="Min" />
      <Label layoutX="35.0" layoutY="305.0" text="Company Name" />
   </children>
</AnchorPane>
